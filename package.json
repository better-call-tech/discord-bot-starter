{
  "name": "discord-bot-boilerprint",
  "version": "1.0.0",
  "description": "A Discord bot boilerprint with TypeScript",
  "type": "module",
  "author": {
    "email": "slahrach.me@gmail.com",
    "name": "sara lahrach"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/slahrach/discord-bot-boilerprint.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write src/**/*.ts",
    "compile": "tsc && npm run copy",
    "clean": "rimraf dist/",
    "copy": "npx copyfiles .env src/**/*.json src/**/*.js dist/",
    "build": "npm run clean && npm run compile",
    "start": "npm run build &&node dist/index.js",
    "dev": "tsc-watch --onSuccess \"npm run start\"",
    "prod": "npm run build && npm run start",
    "prepare": "husky"
  },
  "dependencies": {
    "@discordjs/rest": "^2.0.1",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "husky": "9.1.6",
    "@prisma/client": "^5.19.1",
    "@tsconfig/node20": "^20.1.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.2",
    "prisma": "^5.19.1",
    "prisma-cli": "^1.0.9",
    "rimraf": "^5.0.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6"
  }
}
